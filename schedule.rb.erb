MONDAY    = []
TUESDAY   = []
WEDNESDAY = []

<% schedule_entries.sort_by {|e| e['Start'] }.each do |entry| %>
<%= entry['Day'].upcase %> << {
  :time        => "<%= entry['StartTime'] %> - <%= entry['EndTime'] %>",
  :title       => <%= entry['Title'].inspect %>,
  :description => '',
  :speaker_id  => 'jrubyconf',
  :talk        => true
}
<% end %>

